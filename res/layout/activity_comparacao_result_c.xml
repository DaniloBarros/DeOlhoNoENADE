    <TextView
        android:id="@+id/cidade2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="154dp"
        android:layout_y="158dp"
        android:text="cidade2" />

    <TextView
        android:id="@+id/nota1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="39dp"
        android:layout_y="230dp"
        android:text="nota1"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/nota2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="178dp"
        android:layout_y="229dp"
        android:text="nota2"
        android:textAppearance="?android:attr/textAppearanceLarge" />

</AbsoluteLayout>