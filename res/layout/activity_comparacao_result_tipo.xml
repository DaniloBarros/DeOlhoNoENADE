<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="br.unb.deolhonoenade.view.ComparacaoResultTipo"
    tools:ignore="MergeRootFrame" >


    <TextView
        android:id="@+id/manualRanking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:text="Estado/tipo 1"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <br.unb.br.deolhonoenade.graphs.holographlibrary.BarGraph
        android:id="@+id/graph3"
        android:layout_width="250dp"
        android:layout_height="200dp"
        android:layout_marginLeft="50dp"
	    android:layout_centerHorizontal="true"        
        android:layout_marginTop="30dp" />

<TextView
    android:id="@+id/textView1"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Estado/tipo 1"
    android:layout_marginTop="30dp"
    android:textAppearance="?android:attr/textAppearanceMedium" />


    <TextView
        android:id="@+id/estadoTipo1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/manualRanking"
        android:layout_marginTop="20dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView

        android:id="@+id/OrganizacaoAcademica"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/estadoTipo1"
        android:layout_marginTop="20dp"
        android:text="Estado/tipo 2"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/estadoTipo2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/OrganizacaoAcademica"
        android:layout_marginTop="20dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall" />

</LinearLayout>
