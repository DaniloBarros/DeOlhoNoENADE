<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="br.unb.deolhonoenade.view.ComparacaoResultIES"
    tools:ignore="MergeRootFrame" >

  <TextView
        android:id="@+id/stringCurso"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="Resultado - Comparacao Instituicoes"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="25dp"
        android:textStyle="bold"  />

  <br.unb.br.deolhonoenade.graphs.holographlibrary.BarGraph
      android:id="@+id/graph"
      android:layout_width="250dp"
      android:layout_height="200dp"
	  android:layout_centerHorizontal="true"
      android:layout_below="@+id/stringCurso"
      android:layout_marginTop="15dp" />

  <TextView
      android:id="@+id/Instiruicao2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignParentLeft="true"
      android:layout_below="@+id/nomeIES1"
      android:layout_marginTop="15dp"
      android:text="@string/text_instituicao2"
      android:textAppearance="?android:attr/textAppearanceMedium" />

  <TextView
      android:id="@+id/nomeIES2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignLeft="@+id/nomeIES1"
      android:layout_alignParentRight="true"
      android:layout_below="@+id/Instiruicao2"
      android:layout_marginTop="15dp"
      android:text="Large Text"
      android:textAppearance="?android:attr/textAppearanceSmall" />

  <TextView
      android:id="@+id/Instiruicao1"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignParentLeft="true"
      android:layout_below="@+id/graph"
      android:text="@string/text_instituicao1"
      android:layout_marginTop="15dp"
      android:textAppearance="?android:attr/textAppearanceMedium" />

  <TextView
      android:id="@+id/nomeIES1"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_below="@+id/Instiruicao1"
      android:layout_marginTop="15dp"
      android:text="Large Text"
      android:textAppearance="?android:attr/textAppearanceSmall" />
    
</RelativeLayout>