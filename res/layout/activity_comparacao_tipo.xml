<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="br.unb.deolhonoenade.view.ComparacaoTipo"
    tools:ignore="MergeRootFrame" >

    <Button
        android:id="@+id/comparaT1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="24dp"
        android:text="@string/button_Buscar" />

    <Spinner
        android:id="@+id/Tipo1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/comparaT1" 
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="66dp" />

    <Spinner
        android:id="@+id/EstadoT1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/Tipo1"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="77dp" />

    <TextView
        android:id="@+id/EstadoTipoInicial1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/EstadoT1"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="40dp"
        android:text="Escolha o primeiro Estado" />
    

    <TextView
        android:id="@+id/EstadoTipoInicial2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:text="Escolha o primeiro Tipo" />
    
</RelativeLayout>

